tar -xf $ARCHIVE_DIR/$ARCHIVE
cd mono-debugger-2.10
./configure --prefix=$MONO_PREFIX
make

sudo checkinstall -D \
  --fstrans=no \
  --pkgname pmono-mono-debugger \
  --pkgaltsource $URL \
