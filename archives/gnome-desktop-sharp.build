#!/bin/bash
set -e

tar -xf $ARCHIVE_DIR/$ARCHIVE
cd gnome-desktop-sharp-2.24.0
./configure --prefix=$MONO_PREFIX
make

PNAME=pmono-gnome-desktop-sharp2
PVERS=2.24.0
PDEPS=pmono-mono,pmono-gtk-sharp2,pmono-gnome-sharp2,libgtksourceview2.0-dev,libwnck-dev,librsvg2-dev,libgtkhtml3.14-dev

../../pmono_pkg $PNAME $PVERS $PDEPS "$GLOBAL_MAINTAINER" $URL 

set +e
