#!/bin/bash
set -e

if [ "x" == "x$NO_BUILD" ] 
then

	tar -xf "$ARCHIVE_DIR/$ARCHIVE"
fi
cd mono-addins-0.6.2

if [ "x" == "x$NO_BUILD" ] 
then
	./configure --prefix=$MONO_PREFIX
	make
fi

export PNAME=pmono-mono-addins
export PVERS=0.6.2
export PDEPS=pmono-mono,pmono-gtk-sharp2,pmono-mono-tools


set +e
