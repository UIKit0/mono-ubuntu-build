#!/bin/bash
set -e

cd mono-master

export PNAME=pmono-mono
export PVERS=3.0.10
export PDEPS=pmono-libgdiplus


if [ "x" == "x$NO_BUILD" ] 
then

	rm -rf *
        git reset --hard
	./autogen.sh --prefix=$MONO_PREFIX
        make get-monolite-latest
        make EXTERNAL_MCS=${PWD}/mcs/class/lib/monolite/gmcs.exe
fi

set +e
